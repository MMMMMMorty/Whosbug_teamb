--langdef=Swift
--langmap=Swift:.swift
--regex-swift=/(private|public|protect)[\(\w+\) \t]+(var|let)[ \t]+([^:=]+).*$/\3/v,variable/
--regex-swift=/func[ \t]+([^\(\)]+)\([^\(\)]*\)/\1/f,function/
--regex-swift=/(init)[ \t|\?\t]?(\([^\(\)]*\))?( )*\{/\1/i,initFunction/
--regex-swift=/class[ \t]+([^:\{]+).*$/\1/c,class/
--regex-swift=/protocol[ \t]+([^:\{]+).*$/\1/p,protocol/